<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="dataInputRoot" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
    <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <TextField fx:id="givenNameTF" prefHeight="30.0" promptText="%givenName" >
                <padding>
                    <Insets left="2.0" right="2.0"/>
                </padding>
                <GridPane.margin>
                    <Insets left="20.0" right="20.0"/>
                </GridPane.margin>
                <font>
                    <Font size="14.0"/>
                </font>
            </TextField>
            <TextField fx:id="familyNameTF" layoutX="160.0" layoutY="83.0" prefHeight="30.0"
                       promptText="%familyName" GridPane.rowIndex="1">
                <padding>
                    <Insets left="2.0" right="2.0"/>
                </padding>
                <GridPane.margin>
                    <Insets left="20.0" right="20.0"/>
                </GridPane.margin>
                <font>
                    <Font size="14.0"/>
                </font>
            </TextField>
            <TextField fx:id="yearOfBirthTF" layoutX="160.0" layoutY="117.0" maxWidth="80.0"
                       prefHeight="30.0" prefWidth="60.0" promptText="%yearOfBirth"
                       GridPane.halignment="CENTER" GridPane.rowIndex="2">
                <padding>
                    <Insets left="2.0" right="2.0"/>
                </padding>
                <GridPane.margin>
                    <Insets left="20.0" right="20.0"/>
                </GridPane.margin>
                <font>
                    <Font size="14.0"/>
                </font>
            </TextField>
            <HBox alignment="CENTER" spacing="5.0" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                  GridPane.rowIndex="3" GridPane.valignment="CENTER">
                <Button fx:id="saveButton" defaultButton="true" mnemonicParsing="false" text="%save"
                        GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <Button fx:id="cancelButton" cancelButton="true" layoutX="10.0" layoutY="10.0"
                        mnemonicParsing="false" text="%cancel"/>
            </HBox>
        </GridPane>
    </StackPane>
</AnchorPane>
